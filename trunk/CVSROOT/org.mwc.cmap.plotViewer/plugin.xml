<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.mwc.cmap.plotViewer"
   name="Plot Viewer"
   version="1.0.0"
   provider-name="MWC"
   class="org.mwc.cmap.plotViewer.PlotViewerPlugin">

   <runtime>
      <library name="plotViewer.jar">
         <export name="*"/>
      </library>
   </runtime>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.mwc.cmap.legacy"/>
      <import plugin="org.mwc.cmap.core"/>
   </requires>

   <extension
         point="org.eclipse.ui.editors">
      <editor
            name="2D Plot Editor"
            extensions="xml"
            icon="icons/sample.gif"
            class="org.mwc.cmap.plotViewer.editors.PlotEditor"
            id="org.mwc.cmap.plotViewer.editors.PlotEditor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="org.mwc.cmap.plotViewer.DebriefCategory"
            name="Debrief"/>
   </extension>

</plugin>
