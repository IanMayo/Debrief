<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:my="urn:import:com.planetmayo.debrief.satc.gwt.client.ui">
	<ui:style>

	</ui:style>
	<g:HTMLPanel>
		<g:CaptionPanel captionText="Alteration Leg Forecast - Turn to south">

			<g:DisclosurePanel animationEnabled="true" open="true"
				width="100%">

				<g:customHeader>
					<my:ContributionPanelHeader active="true"
						weighting="0" hardConstraints="N/A" estimate="N/A"></my:ContributionPanelHeader>

				</g:customHeader>

				<g:HTMLPanel styleName="detail-panel aleteration-leg-forecast">
					<g:CaptionPanel captionText="Adjust">
						<g:HTMLPanel>
							<my:NameWidget></my:NameWidget>
							<my:StartFinishWidget></my:StartFinishWidget>

							<my:Slider2BarWidget ui:field="maxCourse" type="course" text="Max Course Change"
								sliderWidth="160px"></my:Slider2BarWidget>
							<my:Slider2BarWidget ui:field="maxSpeed" type="speed" text="Max Speed Change"
								sliderWidth="160px"></my:Slider2BarWidget>

						</g:HTMLPanel>
					</g:CaptionPanel>
				</g:HTMLPanel>

			</g:DisclosurePanel>

		</g:CaptionPanel>
	</g:HTMLPanel>
</ui:UiBinder> 