<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:my="urn:import:com.planetmayo.debrief.satc.gwt.client.ui">
	<ui:style>

	</ui:style>
	<g:HTMLPanel>
		<g:CaptionPanel captionText="Speed Forecast - Easterly leg">

			<g:DisclosurePanel animationEnabled="true" open="true"
				width="100%">

				<g:customHeader>
					<my:ContributionPanelHeader active="true" ui:field="header"
						weighting="9" hardConstraints="3-11 kts" estimate="6 kts"></my:ContributionPanelHeader>

				</g:customHeader>

				<g:HTMLPanel styleName="detail-panel course-forecast">
					<g:CaptionPanel captionText="Adjust">
						<g:HTMLPanel>
							<my:NameWidget ui:field="name"></my:NameWidget>
							<my:StartFinishWidget ui:field="startFinish"></my:StartFinishWidget>

							<my:Slider2BarWidget ui:field="min" type="speed" text="Min :"></my:Slider2BarWidget>
							<my:Slider2BarWidget ui:field="max" type="speed" text="Max :"></my:Slider2BarWidget>
							<my:Slider2BarWidget ui:field="estimate" type="speed" text="Estimate :"></my:Slider2BarWidget>
						</g:HTMLPanel>
					</g:CaptionPanel>
				</g:HTMLPanel>

			</g:DisclosurePanel>

		</g:CaptionPanel>
	</g:HTMLPanel>
</ui:UiBinder> 