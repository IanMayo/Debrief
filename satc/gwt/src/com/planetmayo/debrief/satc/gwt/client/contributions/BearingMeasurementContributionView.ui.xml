<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:my="urn:import:com.planetmayo.debrief.satc.gwt.client.ui">
	<ui:style>

	</ui:style>
	<g:HTMLPanel>
		<g:CaptionPanel captionText="Bearing Measurement - First Period">

			<g:DisclosurePanel animationEnabled="true" open="true"
				width="100%">

				<g:customHeader>
					<my:ContributionPanelHeader active="true" ui:field="header"
						weighting="9" hardConstraints="+/- 3&deg;" estimate="45 Measurements"></my:ContributionPanelHeader>

				</g:customHeader>

				<g:HTMLPanel styleName="detail-panel">
					<g:CaptionPanel captionText="Adjust">
						<g:HTMLPanel>
							<my:NameWidget></my:NameWidget>

							<my:StartFinishWidget loaded="true"></my:StartFinishWidget>

							<my:Slider2BarWidget type="bearing" text="Limit:"></my:Slider2BarWidget>

						</g:HTMLPanel>
					</g:CaptionPanel>
				</g:HTMLPanel>

			</g:DisclosurePanel>

		</g:CaptionPanel>
	</g:HTMLPanel>
</ui:UiBinder> 