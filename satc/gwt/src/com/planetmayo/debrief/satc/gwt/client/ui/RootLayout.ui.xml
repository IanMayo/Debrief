<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:my="urn:import:com.planetmayo.debrief.satc.gwt.client.ui">
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more
		details */
		.important {
		font-weight: bold;
		}

		.root-layout {
		position: absolute;
		left: 0px;
		right: 0px;
		bottom: 0px;
		top: 0px;
		}
	</ui:style>
	<g:HTMLPanel styleName="{style.root-layout}">
		<g:HorizontalSplitPanel width="100%" height="100%"
			splitPosition="475px">

			<g:VerticalSplitPanel width="100%" height="100%"
				splitPosition="150px">

				<my:TestHarness></my:TestHarness>
				<my:ManageSolutions></my:ManageSolutions>

			</g:VerticalSplitPanel>

			<g:Label text=" "></g:Label>


		</g:HorizontalSplitPanel>

	</g:HTMLPanel>
</ui:UiBinder> 