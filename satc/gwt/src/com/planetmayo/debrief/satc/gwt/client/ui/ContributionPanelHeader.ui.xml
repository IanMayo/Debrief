<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:my="urn:import:com.planetmayo.debrief.satc.gwt.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:HorizontalPanel horizontalAlignment="center"
			ui:field="header" width="100%">
			<g:FlowPanel styleName="disclosurePanel-img"></g:FlowPanel>
			<g:CheckBox ui:field="active"></g:CheckBox>
			<g:InlineLabel ui:field="hardConstraints" text=""></g:InlineLabel>
			<g:InlineLabel ui:field="estimate" text="180 degs"></g:InlineLabel>
			<my:NumberSpinner ui:field="weighting"></my:NumberSpinner>
		</g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 