<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:my="urn:import:com.planetmayo.debrief.satc.gwt.client.ui">
	<ui:style>

	</ui:style>
	<g:HTMLPanel>
		<g:CaptionPanel captionText="Location Forecast - Ship Logs">

			<g:DisclosurePanel ui:field="disclosurePanel" animationEnabled="false" open="true"
				width="100%">

				<g:customHeader>
					<my:ContributionPanelHeader active="true" ui:field="header"
						weighting="9" hardConstraints="2000m" estimate="12.2N 3.4W"></my:ContributionPanelHeader>


				</g:customHeader>

				<g:HTMLPanel styleName="detail-panel location-forecast">
					<g:CaptionPanel captionText="Adjust">
						<g:HTMLPanel>
							<my:NameWidget ui:field="name"></my:NameWidget>
							<my:StartFinishWidget ui:field="startFinish"></my:StartFinishWidget>

							<my:Slider2BarWidget ui:field="limit" type="location" text="Distance:" sliderWidth="190px"></my:Slider2BarWidget>
							<my:NameWidget ui:field="estimate" text="Location: "></my:NameWidget>

						</g:HTMLPanel>
					</g:CaptionPanel>
				</g:HTMLPanel>

			</g:DisclosurePanel>

		</g:CaptionPanel>
	</g:HTMLPanel>
</ui:UiBinder> 