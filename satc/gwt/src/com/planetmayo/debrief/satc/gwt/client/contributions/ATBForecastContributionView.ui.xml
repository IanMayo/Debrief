<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:my="urn:import:com.planetmayo.debrief.satc.gwt.client.ui">
	<ui:style>

	</ui:style>
	<g:HTMLPanel>
		<g:CaptionPanel captionText="ATB Forecast - Held on Telescope">

			<g:DisclosurePanel ui:field="disclosurePanel" animationEnabled="true" open="true"
				width="100%">

				<g:customHeader>
					<my:ContributionPanelHeader active="true"
						weighting="9" hardConstraints="45&deg;-120&deg;" estimate="80&deg; "></my:ContributionPanelHeader>

				</g:customHeader>

				<g:HTMLPanel styleName="detail-panel course-forecast">
					<g:CaptionPanel captionText="Adjust">
						<g:HTMLPanel>
							<my:NameWidget></my:NameWidget>
							<my:StartFinishWidget></my:StartFinishWidget>

							<my:Slider2BarWidget type="course" text="Min :"></my:Slider2BarWidget>
							<my:Slider2BarWidget type="course" text="Max :"></my:Slider2BarWidget>
							<my:Slider2BarWidget type="course" text="Estimate :"></my:Slider2BarWidget>
						</g:HTMLPanel>
					</g:CaptionPanel>
				</g:HTMLPanel>

			</g:DisclosurePanel>

		</g:CaptionPanel>
	</g:HTMLPanel>
</ui:UiBinder> 